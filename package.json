{
  "name": "helloworld",
  "description": "hello world page",
  "version": "0.0.1",
  "scripts": {
    "test": "nyc mocha --timeout=3000",
    "start": "npm run start:dev",
    "start:dev": "cross-env NODE_ENV=development nodemon server/index.js --ignore client/ --ignore admin/",
    "start:prod": "npm run build:prod && npm run node:prod",
    "clean:dev": "cross-env NODE_ENV=development node utils/clean.js",
    "clean:staging": "cross-env NODE_ENV=staging node utils/clean.js",
    "clean:prod": "cross-env NODE_ENV=production node utils/clean.js",
    "node:prod": "pm2 start config/prod.json",
    "build:prod": "npm run webpack:prod",
    "webpack:prod": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "webpack:dev": "cross-env NODE_ENV=development webpack --progress --hide-modules"
  },
  "dependencies": {
    "body-parser": "^1.17.2",
