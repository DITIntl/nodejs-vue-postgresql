    "helmet-csp": "^2.5.1",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "pg": "^7.0.2",
    "sequelize": "^4.4.2",
    "vue": "^2.4.2",
    "vue-localstorage": "^0.4.1",
    "vue-router": "^2.7.0",
    "vuex": "^2.3.1",
    "webpack": "^3.4.1",
    "webpack-dev-middleware": "^1.11.0",
    "webpack-hot-middleware": "^2.18.2",
